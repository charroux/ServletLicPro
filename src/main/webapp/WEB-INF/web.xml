<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
<display-name>Data</display-name>

	<servlet>
		<servlet-name>form</servlet-name>
		<servlet-class>form.FormulaireBasique</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ServletVersPremiereJSP</display-name>
		<servlet-name>ServletVersPremiereJSP</servlet-name>
		<servlet-class>form.ServletVersPremiereJSP</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ServletEtudiant</display-name>
		<servlet-name>ServletEtudiant</servlet-name>
		<servlet-class>form.ServletEtudiant</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ServletListeEtudiants</display-name>
		<servlet-name>ServletListeEtudiants</servlet-name>
		<servlet-class>form.ServletListeEtudiants</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ServletCreationFormulaire</display-name>
		<servlet-name>ServletCreationFormulaire</servlet-name>
		<servlet-class>form.ServletCreationFormulaire</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>form</servlet-name>
		<url-pattern>/formulaireURL</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletVersPremiereJSP</servlet-name>
		<url-pattern>/urlServletPremiereJSP</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletEtudiant</servlet-name>
		<url-pattern>/urlEtudiant</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletListeEtudiants</servlet-name>
		<url-pattern>/listEtudiantsURL</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletCreationFormulaire</servlet-name>
		<url-pattern>/urlServletCreationJSP</url-pattern>
	</servlet-mapping>
		
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>